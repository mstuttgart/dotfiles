{
    "editor.renderWhitespace": "all",
    "breadcrumbs.enabled": true,
    "files.autoSave": "afterDelay",
    "files.exclude": {
        "**/__pycache__": true
    },
    "gitProjectManager.baseProjectsFolders": [
        "/home/michell/Workspace"
    ],
    "window.titleBarStyle": "custom",
    "editor.selectionClipboard": false,
